{
    "version": 2,
    "builds": [
        {
            "src": "Frontend/**",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "Frontend/dist"
            }
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Frontend/$1"
        }
    ]
}